{
  "name": "backend-african-marketplace",
  "version": "1.0.0",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "migrate:dev": "knex migrate:latest",
    "rollback:dev": "knex migrate:rollback",
    "seed:dev": "knex seed:run",
    "migrate:prod": "heroku run knex migrate:latest -a backend-african-marketplace",
    "rollback:prod": "heroku run knex migrate:rollback -a backend-african-marketplace",
    "database:prod": "heroku pg:psql -a backend-african-marketplace",
    "seed:prod": "heroku run knex seed:run -a backend-african-marketplace",
    "test": "cross-env NODE_ENV=testing jest --verbose --runInBand",
    "deploy": "git push heroku main"
  },
  "engines": {
    "node": "16.14.2"
  },
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "2.8.5",
    "dotenv": "14.3.0",
    "express": "4.17.2",
    "helmet": "5.0.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "1.0.1",
    "knex-cleaner": "1.3.1",
    "node": "^16.13.2",
    "pg": "8.7.1"
  },
  "devDependencies": {
    "@types/jest": "27.4.0",
    "cross-env": "7.0.3",
    "eslint": "8.7.0",
    "jest": "27.4.7",
    "nodemon": "^2.0.15",
    "supertest": "6.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/African-Market-Place-Jan22/back-end.git"
  }
}
